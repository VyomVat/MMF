<?xml version='1.0' encoding='utf-8'?>
<widget id="NatureHeals.Arcken.Com" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MMF</name>
    <description>
        Multimedia Format Converter
    </description>
    <author email="dev@arcken.com" href="http://nature.arcken.com">
        Arcken Team
    </author>
    <content src="index.html" />
    <icon src="MMF.png" platform="android" width="48" height="48" />
    <access origin="*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
</widget>
